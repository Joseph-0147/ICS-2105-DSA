#include <stdio.h>

int containsDuplicate(int nums[], int numsSize) {
    int seen[100001] = {0};

    for (int i = 0; i < numsSize; ++i) {
        if (seen[nums[i]] == 1) {
            return 1; // True
        }
        seen[nums[i]] = 1;
    }

    return 0; // False
}
main(){
int nums[] = {1, 2, 3, 2};
int numsSize = sizeof(nums) / sizeof(nums[0]);
printf("%s\n", containsDuplicate(nums, numsSize) ? "true" : "false");  // Output: true
return 0;
}