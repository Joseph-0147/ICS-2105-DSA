#include <stdio.h>

int singleNumber(int* nums, int numsSize){
    int single = 0;
    for(int i = 0; i < numsSize; i++){
        single ^= nums[i];
    }
    return single;
}
main(){
int nums[] = {7, 14, 21, 14, 21, 1, 1};
int numsSize = sizeof(nums) / sizeof(nums[0]);
printf("%d\n", singleNumber(nums, numsSize));  // Output: 7
return 0;
}